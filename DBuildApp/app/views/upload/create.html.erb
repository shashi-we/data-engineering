<div class="panel panel-default">
  <div class="panel-heading">
    Uploaded data.
  </div>
  <div class="panel-body">
  <% if @error %>
    Problem loading data! <% @error %>
  <% else %>
    <%= number_to_currency(@total_revenue) %> generated by <%= @counter - 1 %> purchases.
  <% end %>
  </div>
</div>
